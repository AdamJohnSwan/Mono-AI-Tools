    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}/rag_server/src",
            "name": "Rag Server debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "rag_server.main:app",
                "--port", "8001",
                "--reload"
            ],
            "justMyCode": false,
        },
         {
            "cwd": "${workspaceFolder}/agentic-answers",
            "name": "Agentic Answers debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "agentic_answers.main:app",
                "--port", "8001",
                "--reload"
            ],
            "justMyCode": false,
        }
    ]
}